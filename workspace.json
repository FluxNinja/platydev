    "hasura": {
      "root": "apps/hasura",
      "sourceRoot": "apps/hasura",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "options": { "command": "echo \"no linting yet for this project\"" }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "./tools/executors/docker.sh build hasura"
          }
        },
        "deploy": {
          "dependsOn": [
            {
              "target": "build:production",
              "projects": "self"
            }
          ],
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "./tools/executors/docker.sh push hasura"
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "baseBranch": "master"
          }
        }
      }
    },
    "charts-hasura": {
      "root": "charts/hasura",
      "sourceRoot": "charts/hasura",
      "projectType": "library",
      "targets": {
        "prepare": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "helm dependency build charts/hasura",
            "parallel": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "helm package charts/hasura -u -d dist/charts/hasura",
            "parallel": false
          },
          "configurations": {
            "production": {}
          }
        },
        "version": {
          "executor": "./tools/executors/helm:version"
