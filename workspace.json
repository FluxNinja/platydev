{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/next" },
  "defaultProject": "platyplus",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": { "application": { "style": "less", "linter": "eslint" } }
  },
  "projects": {
    "platyplus": {
      "root": "apps/platyplus",
      "sourceRoot": "apps/platyplus",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/platyplus",
            "outputPath": "dist/apps/platyplus"
          },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "platyplus:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "platyplus:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "platyplus:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/platyplus"],
          "options": {
            "jestConfig": "apps/platyplus/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/platyplus/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "platyplus-e2e": {
      "root": "apps/platyplus-e2e",
      "sourceRoot": "apps/platyplus-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/platyplus-e2e/cypress.json",
            "tsConfig": "apps/platyplus-e2e/tsconfig.e2e.json",
            "devServerTarget": "platyplus:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "platyplus:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/platyplus-e2e/**/*.{js,ts}"] }
        }
      }
    },
